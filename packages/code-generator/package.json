{
  "name": "@lowcode/code-generator",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --tsconfig tsconfig.build.json",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --tsconfig tsconfig.build.json"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "@lowcode/schema": "workspace:*",
    "prettier": "^3.1.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "@types/lodash-es": "^4.17.0",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.0.0"
  }
}
